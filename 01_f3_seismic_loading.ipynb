import numpy as np
import matplotlib.pyplot as plt
import segyio


segy_file = "data/F3_Netherlands/F3_Seismic.sgy"

with segyio.open(segy_file, "r", ignore_geometry=True) as f:
    f.mmap()
    seismic_cube = segyio.tools.cube(f)

print("Seismic cube shape (Inline, Crossline, Time):", seismic_cube.shape)


inline_index = seismic_cube.shape[0] // 2

plt.figure(figsize=(10, 6))
plt.imshow(
    seismic_cube[inline_index, :, :].T,
    cmap="seismic",
    aspect="auto"
)
plt.colorbar(label="Amplitude")
plt.xlabel("Crossline")
plt.ylabel("Time Samples")
plt.title(f"Inline Section (Inline {inline_index})")
plt.gca().invert_yaxis()
plt.tight_layout()
plt.show()


time_index = seismic_cube.shape[2] // 2

plt.figure(figsize=(8, 6))
plt.imshow(
    seismic_cube[:, :, time_index],
    cmap="seismic"
)
plt.colorbar(label="Amplitude")
plt.xlabel("Inline")
plt.ylabel("Crossline")
plt.title(f"Time Slice (Sample {time_index})")
plt.tight_layout()
plt.show()


rms_amplitude = np.sqrt(np.mean(seismic_cube ** 2, axis=2))

plt.figure(figsize=(8, 6))
plt.imshow(rms_amplitude, cmap="inferno")
plt.colorbar(label="RMS Amplitude")
plt.xlabel("Inline")
plt.ylabel("Crossline")
plt.title("RMS Amplitude Attribute Map")
plt.tight_layout()
plt.show()


print("Seismic data loaded successfully.")
print("Basic seismic interpretation and attribute extraction completed.")
